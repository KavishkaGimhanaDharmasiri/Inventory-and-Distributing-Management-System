{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 123",
    "not and_ff <= 124",
    "not chrome <= 123",
    "not edge <= 123",
    "not firefox <= 124",
    "not opera <= 109",
    "not samsung <= 24",
    "not ios_saf <= 17.4",
    "not safari <= 17.4"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "no-inline-styles": "off"
  }
}